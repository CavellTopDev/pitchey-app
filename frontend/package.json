{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node_modules/.bin/vite build",
    "build:dev": "tsc && vite build",
    "build-with-types": "tsc -b && vite build",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=verbose",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:all": "npm run test:ci && npm run test:e2e",
    "build:analyze": "vite build && node analyze-bundle.js",
    "build:prod": "NODE_ENV=production vite build --mode production",
    "analyze": "node analyze-bundle.js",
    "serve:prod": "vite preview --port 4173",
    "release:sentry": "bash ./scripts/sentry-release.sh",
    "release:sentry:finalize": "SENTRY_RELEASE=${SENTRY_RELEASE:-$(git rev-parse --short HEAD)} sentry-cli releases finalize \"$SENTRY_RELEASE\"",
    "deploy:pages": "vite build --mode production && wrangler pages deploy dist",
    "deploy:pages:with-sentry": "SENTRY_RELEASE=${SENTRY_RELEASE:-$(git rev-parse --short HEAD)} npm run release:sentry && SENTRY_RELEASE=$SENTRY_RELEASE vite build --mode production && wrangler pages deploy dist && npm run release:sentry:finalize"
  },
  "dependencies": {
    "@sentry/react": "^10.25.0",
    "@sentry/replay": "^7.116.0",
    "@sentry/vite-plugin": "^4.6.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.18",
    "@tanstack/react-query": "^5.89.0",
    "axios": "^1.12.2",
    "chart.js": "^4.5.1",
    "date-fns": "^4.1.0",
    "html-to-image": "^1.11.11",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^4.0.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "jsdom": "^27.0.1",
    "msw": "^2.11.6",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.6",
    "vitest": "^4.0.2"
  }
}
