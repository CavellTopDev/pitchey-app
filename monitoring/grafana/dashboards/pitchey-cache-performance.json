{
  "dashboard": {
    "id": null,
    "title": "Pitchey Cache Performance Dashboard",
    "tags": ["cloudflare", "cache", "performance", "pitchey"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Overall Cache Hit Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(cloudflare_cache_hits) / sum(cloudflare_cache_requests) * 100",
            "legendFormat": "Cache Hit Rate"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 80}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 2,
        "title": "Cache Warming Effectiveness",
        "type": "stat",
        "targets": [
          {
            "expr": "pitchey_cache_warmer_success_rate",
            "legendFormat": "Warming Success Rate"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 95}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 3,
        "title": "Average Cache Latency",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(pitchey_cache_response_time_ms)",
            "legendFormat": "Avg Cache Latency"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            },
            "unit": "ms"
          }
        }
      },
      {
        "id": 4,
        "title": "Cache Evictions/hour",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(pitchey_cache_evictions_total[1h]) * 3600",
            "legendFormat": "Evictions/hour"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 500}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Cache Hit Rate by Endpoint",
        "type": "table",
        "targets": [
          {
            "expr": "sum by (endpoint) (cloudflare_cache_hits) / sum by (endpoint) (cloudflare_cache_requests) * 100",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {"h": 10, "w": 12, "x": 0, "y": 8},
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "renameByName": {"endpoint": "Endpoint", "Value": "Hit Rate %"}
            }
          }
        ]
      },
      {
        "id": 6,
        "title": "Cache Performance Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(cloudflare_cache_hits[5m]) / rate(cloudflare_cache_requests[5m]) * 100",
            "legendFormat": "Hit Rate %"
          },
          {
            "expr": "rate(cloudflare_cache_misses[5m]) / rate(cloudflare_cache_requests[5m]) * 100",
            "legendFormat": "Miss Rate %"
          }
        ],
        "gridPos": {"h": 10, "w": 12, "x": 12, "y": 8},
        "yAxes": [
          {
            "label": "Percentage",
            "min": 0,
            "max": 100
          }
        ]
      },
      {
        "id": 7,
        "title": "Cache Latency Distribution",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(pitchey_cache_latency_histogram_bucket[5m])",
            "format": "heatmap",
            "legendFormat": "{{le}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 18}
      },
      {
        "id": 8,
        "title": "Cache Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "pitchey_cache_memory_usage_bytes",
            "legendFormat": "Memory Used"
          },
          {
            "expr": "pitchey_cache_memory_limit_bytes",
            "legendFormat": "Memory Limit"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 18},
        "yAxes": [
          {
            "label": "Bytes",
            "min": 0
          }
        ]
      },
      {
        "id": 9,
        "title": "Cache Operations Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(pitchey_cache_gets_total[1m]) * 60",
            "legendFormat": "Gets/min"
          },
          {
            "expr": "rate(pitchey_cache_sets_total[1m]) * 60",
            "legendFormat": "Sets/min"
          },
          {
            "expr": "rate(pitchey_cache_deletes_total[1m]) * 60",
            "legendFormat": "Deletes/min"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 26},
        "yAxes": [
          {
            "label": "Operations/min",
            "min": 0
          }
        ]
      }
    ]
  }
}