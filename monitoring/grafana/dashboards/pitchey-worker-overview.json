{
  "dashboard": {
    "id": null,
    "title": "Pitchey Worker - Performance Overview",
    "tags": ["cloudflare", "worker", "pitchey"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Worker Requests/min",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(cloudflare_worker_requests_total[1m]) * 60",
            "legendFormat": "Requests/min"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1000},
                {"color": "red", "value": 5000}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Cache Hit Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "cloudflare_cache_hit_rate",
            "legendFormat": "Hit Rate %"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 80}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(cloudflare_worker_errors_total[5m]) / rate(cloudflare_worker_requests_total[5m]) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 4,
        "title": "P95 Response Time",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, cloudflare_worker_duration_seconds)",
            "legendFormat": "P95 Response Time"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 500},
                {"color": "red", "value": 1000}
              ]
            },
            "unit": "ms"
          }
        }
      },
      {
        "id": 5,
        "title": "Request Rate Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(cloudflare_worker_requests_total[1m]) * 60",
            "legendFormat": "Requests/min"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "yAxes": [
          {
            "label": "Requests/min",
            "min": 0
          }
        ]
      },
      {
        "id": 6,
        "title": "Response Time Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, cloudflare_worker_duration_seconds)",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, cloudflare_worker_duration_seconds)",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, cloudflare_worker_duration_seconds)",
            "legendFormat": "P99"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "yAxes": [
          {
            "label": "Response Time (ms)",
            "min": 0
          }
        ]
      },
      {
        "id": 7,
        "title": "Status Code Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (status) (rate(cloudflare_worker_requests_total[5m]))",
            "legendFormat": "{{status}}"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Top Endpoints by Volume",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum by (endpoint) (rate(cloudflare_worker_requests_total[5m])) * 300)",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16},
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "renameByName": {"endpoint": "Endpoint", "Value": "Requests/5min"}
            }
          }
        ]
      },
      {
        "id": 9,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "cloudflare_worker_cpu_time_seconds",
            "legendFormat": "CPU Time"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16},
        "yAxes": [
          {
            "label": "CPU Time (seconds)",
            "min": 0
          }
        ]
      }
    ]
  }
}